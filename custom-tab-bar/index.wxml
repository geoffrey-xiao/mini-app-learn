<!--custom-tab-bar/index.wxml-->
<van-tabbar active="{{ active }}" bind:change="onChange" active-color="#0034ff">
  <van-tabbar-item wx:for="{{list}}" wx:key="index" info="{{item.info}}">
    <image slot="icon" src="{{ item.iconPath }}" mode="aspectFit" style="width: 25px; height: 25px;" />
    <image slot="icon-active" mode="aspectFit" src="{{ item.selectedIconPath}}" style="width: 25px; height: 25px;" />
    {{item.text}}
  </van-tabbar-item>
</van-tabbar>